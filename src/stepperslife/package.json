    "react-zoom-pan-pinch": "^3.7.0",
    "resend": "^6.4.2",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "square": "^43.2.0",
    "stripe": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",

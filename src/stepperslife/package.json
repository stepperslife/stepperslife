{
  "name": "stepperslife",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3004",
    "build": "npx convex deploy --cmd 'next build'",
    "start": "next start -p 3000",
    "lint": "eslint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prepare": "husky",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report test-results/html-report",
    "test:payment:all": "ts-node scripts/run-comprehensive-payment-tests.ts all",
    "test:payment:required": "ts-node scripts/run-comprehensive-payment-tests.ts required",
    "test:payment:api": "ts-node scripts/test-payment-api.ts",
    "test:payment:e2e": "playwright test tests/comprehensive-payment-suite.spec.ts",
    "test:payment:verify": "ts-node scripts/verify-split-payments.ts application-fees",
    "test:payment:cleanup": "ts-node scripts/cleanup-test-data.ts stats",
    "test:payment:cleanup:all": "ts-node scripts/cleanup-test-data.ts all"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@auth/core": "^0.34.3",
    "@google/generative-ai": "^0.24.1",
    "@paypal/paypal-server-sdk": "^2.0.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.22.0",
    "@square/web-payments-sdk-types": "^1.79.0",
    "@stripe/react-stripe-js": "^5.3.0",
    "@stripe/stripe-js": "^8.2.0",
    "@types/lodash": "^4.17.20",
    "@types/qrcode": "^1.5.6",
    "bcryptjs": "^3.0.2",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.28.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.24",
    "html5-qrcode": "^2.3.8",
    "jose": "^6.1.1",
    "jsqr": "^1.4.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.546.0",
    "next": "^16.0.3",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "openai": "^6.7.0",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "19.2.0",
